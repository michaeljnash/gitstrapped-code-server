{
  "settings": {
    "someSettingKey": "someSettingVal",
    "workbench.colorTheme": "Monokai"
  },
  "keybindings": [
    {
      "key": "ctrl+7",
      "command": "workbench.action.files.openFile"
    }
  ],
  "tasks": {
    "tasks": [
      {
        "label": "Echo User Input",
        "type": "shell",
        "command": "echo \"You entered: ${input:myPromptString}\"",
        "problemMatcher": []
      }
    ],
    "inputs": [
      {
        "id": "myPromptString",
        "type": "promptString",
        "description": "Please enter some text:"
      }
    ]
  },
  "extensions": [
    "dbaeumer.vscode-eslint"
  ],
  "github": {
    "username": "michaeljnash",
    "repos": [
      "michaeljnash/jekyll-asset-management"
    ],
    "repos-subdir": "/repos",
    "pull-existing": true
  }
}